<div class="container">
    <h1>All Collectors</h1>

    <div class="list">
        {{#each users}}
        <div class="outer">
            <a href="/users/{{this._id}}">
                <div class="collectorBox" style="background-image: url({{this.refAddressOne}})">
                    <span>{{this.name}}</span>
                </div>
            </a>
        </div>
        {{/each}}
    </div>

    <a href='/users/new'><button>Add Collector</button></a>
</div>